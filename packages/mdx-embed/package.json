{
  "name": "mdx-embed",
  "version": "0.0.1",
  "description": "The new MDX Embed",
  "main": "dist/main.js",
  "scripts": {
    "prettier": "prettier --config ../../.prettierrc.js --write \"**/*.{json,js,ts,tsx}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --quiet",
